{
    "version": 2,
    "projects": {
      "bank_webhook_handler": {
        "src": "apps/bank_webhook_handler",
        "buildCommand": "npm install && npm run build",
        "outputDirectory": "dist",
        "routes": [
          {
            "src": "/exp/(.*)",
            "dest": "apps/bank_webhook_handler/$1"
          }
        ]
      },
      "user-app": {
        "src": "apps/user-app",
        "buildCommand": "npm install && npm run build",
        "outputDirectory": ".next",
        "routes": [
          {
            "handle": "filesystem"
          },
          {
            "src": "/(.*)",
            "dest": "apps/user-app/$1"
          }
        ]
      }
    }
  }
  